<%- include("partials/header"); -%>
<h1>Home</h1>
<p> <%= startingcontent %> </p>
<% posts.forEach(function(post){ %>
  <h1> <%= post.title %> </h1>
  <% const myString = post.content.substring(0,100); %>
  <p> <%= myString + " ..." %> <a href="/posts/<%= post.title %>">Read More</a> </p>
<% }); %>
<%- include("partials/footer"); -%>
